
@using WebApplication1.Models
@model IEnumerable<WebApplication1.Models.Employee>
<h1>Employeeeeeeee!</h1>

@{
    <table>
        <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Salary</th>
            <th>Department</th>
        </tr>
        </thead> 
        <tbody>
        
        @foreach (var e in Model)
        {
            <tr>
                <td>@e.Id</td>
                <td>@e.Name</td>
                <td>@e.Salary</td>
                <td>@e.DepartmentId</td>
            </tr>
        }
        </tbody>
    </table>
    
}










